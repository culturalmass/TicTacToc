{
    "name": "tic-tac-toe",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "yarn prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "yarn prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
        "prepare": "husky install"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": "eslint",
        "**/*.{js,jsx,ts,tsx}": "prettier --write"
    },
    "dependencies": {
        "@expo-google-fonts/delius-unicase": "^0.2.2",
        "@react-native-async-storage/async-storage": "~1.17.3",
        "@react-native-community/netinfo": "9.3.0",
        "@react-navigation/native": "^6.0.13",
        "@react-navigation/native-stack": "^6.9.0",
        "@twotalltotems/react-native-otp-input": "1.3.7",
        "@types/zen-observable": "^0.8.3",
        "aws-amplify": "^4.3.37",
        "aws-amplify-react-native": "^6.0.5",
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "expo": "~46.0.13",
        "expo-av": "~12.0.4",
        "expo-device": "~4.3.0",
        "expo-font": "~10.2.0",
        "expo-haptics": "~11.3.0",
        "expo-linear-gradient": "~11.4.0",
        "expo-notifications": "~0.16.1",
        "expo-status-bar": "~1.4.0",
        "graphql": "^16.6.0",
        "graphql-tag": "^2.12.6",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-native": "0.69.6",
        "react-native-modal": "^13.0.1",
        "react-native-safe-area-context": "4.3.1",
        "react-native-screens": "~3.15.0",
        "react-native-web": "~0.18.7",
        "zen-observable": "^0.8.15"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~18.0.14",
        "@types/react-native": "~0.69.1",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint-plugin-react": "^7.31.8",
        "husky": "^8.0.0",
        "lint-staged": "^13.0.3",
        "prettier": "2.7.1",
        "typescript": "~4.3.5"
    },
    "private": true
}
