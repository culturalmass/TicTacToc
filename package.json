{
    "name": "tic-tac-toe",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "typecheck": "tsc",
        "format:check": "yarn prettier --check \"**/*.{js,jsx,json,ts,tsx}\"",
        "format": "yarn prettier --write \"**/*.{js,jsx,json,ts,tsx}\"",
        "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run typecheck && yarn run format:check && yarn run lint"
        }
    },
    "dependencies": {
        "eslint": "^8.24.0",
        "eslint-config-prettier": "^8.5.0",
        "expo": "~46.0.13",
        "expo-status-bar": "~1.4.0",
        "husky": "^8.0.1",
        "react": "18.0.0",
        "react-dom": "18.0.0",
        "react-native": "0.69.6",
        "react-native-web": "~0.18.7"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~18.0.14",
        "@types/react-native": "~0.69.1",
        "@typescript-eslint/eslint-plugin": "^5.39.0",
        "@typescript-eslint/parser": "^5.39.0",
        "eslint-plugin-react": "^7.31.8",
        "prettier": "2.7.1",
        "typescript": "~4.3.5"
    },
    "private": true
}
